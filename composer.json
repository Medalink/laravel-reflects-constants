{
  "name": "medalink/laravel-reflects-constants",
  "type": "library",
  "description": "A small helper package that aids in the retrieval of class constants of models in laravel.",
  "keywords": [
    "Constants",
    "Laravel",
    "PHP"
  ],
  "homepage": "https://github.com/Medalink/laravel-reflects-constants",
  "license": "MIT",
  "authors": [
    {
      "name": "Eric Percifield",
      "email": "medalink7@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "illuminate/support": "^5.6",
    "php" : "^7.1.3"
  },
  "autoload": {
    "psr-4": {
      "Medalink\\Reflects\\": "src"
    }
  },
  "config": {
    "sort-packages": true,
    "minimum-stability": "dev",
    "prefer-stable": false
  }
}